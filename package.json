{
  "name": "faker",
  "description": "Generate massive amounts of fake contextual data",
  "version": "5.5.3",
  "contributors": [
    "Marak Squires <marak.squires@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/faker-js/faker.git"
  },
  "scripts": {
    "browser": "./node_modules/.bin/gulp browser",
    "jsdoc": "./node_modules/.bin/gulp jsdoc",
    "format": "prettier --write .",
    "lint": "node_modules/.bin/eslint ./lib/animal.js",
    "test": "node_modules/.bin/mocha test/*.*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "coveralls": "^3.1.0",
    "eslint": "^6.5.1",
    "gulp": "^4.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-mustache": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "ink-docstrap": "1.1.4",
    "jsdoc": "^3.4.0",
    "lint-staged": "^9.4.2",
    "lodash": "^4.6.1",
    "mocha": "^8.1.1",
    "node-minify": "*",
    "nyc": "^15.1.0",
    "optimist": "0.3.5",
    "prettier": "2.5.1",
    "sinon": "^9.0.3",
    "through2": "2.0.0",
    "typescript": "^4.5.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vinyl-transform": "^1.0.0",
    "vitepress": "^0.21.4"
  },
  "license": "MIT",
  "main": "index.js"
}
